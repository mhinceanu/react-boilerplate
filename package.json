{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "classnames": "^2.5.1",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.1.5",
    "react-scripts": "5.0.1",
    "redux-requests": "^1.0.2",
    "sass": "^1.85.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:update": "react-scripts test --updateSnapshot",
    "coverage": "react-scripts test -- --coverage",
    "eject": "react-scripts eject",
    "beautify": "prettier --write \"src/**/*.js\"",
    "eslint": "eslint ./ --no-cache",
    "prepare": "husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npm run beautify",
      "npm run eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.20.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jest-css-modules": "^2.1.0",
    "lint-staged": "^15.4.3",
    "prettier": "3.5.1"
  }
}
